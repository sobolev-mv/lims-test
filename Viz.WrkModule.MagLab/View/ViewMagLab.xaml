<ruc:RibbonUserControl x:Class="Viz.WrkModule.MagLab.ViewMagLab" 
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon" 
xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" 
xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys"
xmlns:dxl="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol" 
xmlns:ruc="clr-namespace:Smv.RibbonUserUI;assembly=Smv.Prj.Core"
xmlns:conv="clr-namespace:Viz.WrkModule.MagLab;assembly="
xmlns:cnv="clr-namespace:Smv.XAML.Convertors;assembly=Smv.Prj.Core" 
xmlns:sys="clr-namespace:System;assembly=mscorlib" 
RegName="UcMagLab" Caption="Магнитная лаборатория">
  <ruc:RibbonUserControl.Resources>
    <Style x:Key="CellStyle1" TargetType="dxg:GridControl">
      <Style.Triggers>
        <DataTrigger>
          <Setter Property="Background" Value="Red"/>
        </DataTrigger>
      </Style.Triggers>
    </Style>
    <conv:IntToImageConverter x:Key="IntToImageConverter" />
    <conv:SlImageConverter x:Key="SlImageConverter" />
    <cnv:CharYn2BooleanConverter x:Key="CharYn2BooleanConverter" />
  </ruc:RibbonUserControl.Resources>

  <ruc:RibbonUserControl.BarManagerItems>
    <!--dxb:BarButtonItem Name="btnXSamplesRowChanged" Command="{Binding SamplesRowChangedCommand}" /-->

    <dxb:BarEditItem Name="beiSampleDateBegin" Content="Дата Н:" EditValue="{Binding DateSamplesBegin}" Hint="Начальная дата для выборки образцов" EditWidth="90">
      <dxb:BarEditItem.EditSettings>
         <dxe:DateEditSettings TextWrapping="Wrap" IsTextEditable="False" />
      </dxb:BarEditItem.EditSettings>
    </dxb:BarEditItem>

    <dxb:BarEditItem Name="beiSampleDateEnd" Content="Дата К:" EditValue="{Binding DateSamplesEnd}" Hint="Конечная даты для выборки образцов" EditWidth="90">
      <dxb:BarEditItem.EditSettings>
        <dxe:DateEditSettings TextWrapping="Wrap" IsTextEditable="False" />
      </dxb:BarEditItem.EditSettings>
    </dxb:BarEditItem>

    <dxb:BarEditItem Name="beiShift" Content="Смена:" EditValue="{Binding SelectedShift}" Hint="Выбор смены" EditWidth="90">
      <dxb:BarEditItem.EditSettings>
        <dxe:ComboBoxEditSettings ItemsSource="{Binding Path=(dxb:BarEditItem.BarEditItem).DataContext.Shift, RelativeSource={RelativeSource Self}}" DisplayMember="NameShift" ValueMember="Id" IsTextEditable="False"/>
      </dxb:BarEditItem.EditSettings>
    </dxb:BarEditItem>

    <dxb:BarEditItem Name="beiSelectFindMode" Content="Искать по:"  EditValue="{Binding FindSampleMode}" Hint="Выбор критерия поиска образцов" EditWidth="190">
      <dxb:BarEditItem.EditSettings>
        <dxe:ComboBoxEditSettings ItemsSource="{Binding Path=(dxb:BarEditItem.BarEditItem).DataContext.FindMode, RelativeSource={RelativeSource Self}}" DisplayMember="NameMode" ValueMember="Id" IsTextEditable="False">
        </dxe:ComboBoxEditSettings>
      </dxb:BarEditItem.EditSettings>
    </dxb:BarEditItem>

    <dxb:BarEditItem Name="beiTextFind" EditValue="{Binding FindSampleText, Mode=TwoWay}" Content="Значение: " Hint="Ввод значения поиска" EditWidth="190">
      <dxb:BarEditItem.EditSettings>
        <dxe:TextEditSettings TextWrapping="Wrap"/>
      </dxb:BarEditItem.EditSettings>
      <dxb:BarEditItem.EditStyle>
        <Style TargetType="dxe:TextEdit">
          <!--Setter Property="FontSize" Value="14"/-->
          <!--Setter Property="FontWeight" Value="Bold"/-->
        </Style>
      </dxb:BarEditItem.EditStyle>
    </dxb:BarEditItem>

    <dxb:BarEditItem Name="beiSelDevice" EditValue="{Binding SelectedMeasureDevice, Mode=TwoWay}" Content="Устр-во:"  Hint="Выбор измерительного устр-ва" EditWidth="190">
      <dxb:BarEditItem.EditSettings>
        <dxe:ComboBoxEditSettings ItemsSource="{Binding Path=(dxb:BarEditItem.BarEditItem).DataContext.DeviceLst, RelativeSource={RelativeSource Self}}" DisplayMember="Name" ValueMember="Id"  IsTextEditable="False">
        </dxe:ComboBoxEditSettings>
      </dxb:BarEditItem.EditSettings>
    </dxb:BarEditItem>

    <dxb:BarButtonItem Name="btnQuitxyz" Content="Выход" Tag="CloseUserControl" LargeGlyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/close-32x32.png" Description="New blank document." Hint="Закончить работу с АРМ Мaгнитная лаборатория." />
    <dxb:BarButtonItem Name="btnShowSamples" Content="Обр-цы" Command="{Binding GetListSamplesCommand}" LargeGlyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/Samples-32x32.png" Hint="Получить список образцов за выбранный период." />
    <dxb:BarButtonItem Name="btnFindSamples" Content="Найти обр-цы" KeyGesture="Enter" Command="{Binding FindSamplesCommand}" LargeGlyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/Find-32x32.png" Hint="Найти образцы по выбранному критерию." />
    <dxb:BarButtonItem Name="btnSaveData" Content="Сохр." Command="{Binding SaveDataCommand}" LargeGlyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/save-32x32.png" Hint="Сохранить измененные данные в БД." />
    <dxb:BarButtonItem Name="btnMeasureSample" Content="Измер-е обр-ца" Command="{Binding MeasureSampleDataCommand}" LargeGlyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/MeasureSampleUnit-32x32.png" Hint="Получить данные с установки для измерения параметров образца." />
    <dxb:BarButtonItem Name="btnToMes" Content="Обр-ц  в MES" Command="{Binding ToMesCommand}" LargeGlyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/ToMes-32x32.ico" Hint="Отправить характеристики образца в MES-систему." />
    <dxb:BarButtonItem Name="btnToEdit" Content="В измер." Command="{Binding ToBackToEditCommand}" LargeGlyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/SampleToEdit-32x32.png" Hint="Вернуть образец в режим измерений." />
    <dxb:BarButtonItem Name="btnCopyProp" Command="{Binding CopyPropCommand}" Content="Копир. св-ств" LargeGlyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/CopySampleProp-32x32.png" Hint="Копирование свойств с одного образца на другой." />
    <dxb:BarButtonItem Name="btnShowCalcValue" Command="{Binding ShowCalcValuesCommand}" Content="Св-ва пробы" LargeGlyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/ShowCalcValue-32x32.png" Hint="Просмотр свойств пробы." />
    <dxb:BarButtonItem Name="btnDelete" Command="{Binding DeleteSampleCommand}" Content="Удалить обр-ц" LargeGlyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/Delete-32x32.png" Hint="Удаление образца." />
    <dxb:BarButtonItem Name="btnValidate" Command="{Binding ValidateCommand}" Content="Контр." Glyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/Validate-16x16.png" Hint="Включить/выключить проверку заполнения характеристик." />
    <dxb:BarButtonItem Name="btnFr20Import" Command="{Binding Fr20ImportCommand}" Content="Фр20 Импорт" Glyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/Fr20Import-16x16.png" Hint="Импорт данных с Франклина20." />
    <dxb:BarButtonItem Name="btnStatFlag" Command="{Binding ChangeStatFlagCommand}" Content="Искл. ПП" Glyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/StatFlag-16x16.png" Hint="Исключить из отчета ПП."/>
    <dxb:BarButtonItem Name="btnS2LCheck" Command="{Binding CheckS2LCommand}" Content="Пометить Рул АВО" Glyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/Check-32x32.png" Hint="Пометить рулон АВО для обработки лазером."/>
    <dxb:BarButtonItem Name="btnS2LUnCheck" Command="{Binding UnCheckS2LCommand}" Content="Снять пом Рул АВО" Glyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/Delete-32x32.png" Hint="Снять пометку у рулона АВО о обработке лазером."/>
    <dxb:BarButtonItem Name="btnS2LCpy" Command="{Binding CopyPropS2LCommand}" Content="Копир св-ва" Glyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/CopySampleProp-32x32.png" Hint="Скопировать св-ва на конечный рулон обработанный лазером."/>
    <dxb:BarButtonItem Name="btnStZap" Command="{Binding DlgStZapCommand}"  Content="Коэфф-ты" Glyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/DlgStZap-16x16.png" Hint="Настройка коэфф."/>
    <dxb:BarButtonItem Name="btnMesurCof" Command="{Binding DlgMesurCofCommand}" Content="Коррекция" Glyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/DlgMesurCof-16x16.png" Hint="Настройка коррекции измерений."/>
    <dxb:BarButtonItem Name="btnCopyApstProp" Command="{Binding CopyApstPropCommand}" Content="Копир Эпшт" Glyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/CopyApst-16x16.png" Hint="Копировать х-тики Эпштейна."/>
    <dxb:BarButtonItem Name="btnSiemens" Command="{Binding DlgSiemensCommand}" Content="Siemens" Glyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/Slact-16x16.png" Hint="Лист для Siemens."/>
    <dxb:BarButtonItem Name="btnAdgRpt" Command="{Binding AdgRptCommand}" Content="Отч. адг." Glyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/AdgRpt-16x16.png" Hint="Отчет по адгезии."/>
    <dxb:BarButtonItem Name="btnCopyApst4CmpCoil" Command="{Binding CopyApstCmpCoilCommand}" Content="Копир. Эпшт СР" Glyph="pack://application:,,,/Viz.WrkModule.MagLab;Component/Images/CopyApstCmpCoil-16x16.png" Hint="Копирование Эпштейна для составных рулонов."/>
  </ruc:RibbonUserControl.BarManagerItems>

  <ruc:RibbonUserControl.UserPages>
    <dxr:RibbonPage Caption="Образцы">
      <dxr:RibbonPageGroup Name="rpgShow" Caption="Просмотр" AllowCollapse="False">
        <dxb:BarEditItemLink BarItemName="beiSampleDateBegin" />
        <dxb:BarEditItemLink BarItemName="beiSampleDateEnd" />
        <dxb:BarEditItemLink BarItemName="beiShift" />
        <dxb:BarButtonItemLink BarItemName="btnShowSamples" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
      </dxr:RibbonPageGroup>

      <dxr:RibbonPageGroup Name="rpgFind" Caption="Поиск образцов и выбор изм. устр-ва" AllowCollapse="False">
        <dxb:BarEditItemLink BarItemName="beiSelectFindMode" />
        <dxb:BarEditItemLink BarItemName="beiTextFind" />
        <dxb:BarEditItemLink BarItemName="beiSelDevice" />
        <dxb:BarButtonItemLink BarItemName="btnFindSamples" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
      </dxr:RibbonPageGroup>

      <dxr:RibbonPageGroup Name="rpgAction" Caption="Операции" AllowCollapse="False">
        <dxb:BarButtonItemLink BarItemName="btnSaveData" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
        <dxb:BarButtonItemLink BarItemName="btnMeasureSample" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
        <dxb:BarButtonItemLink BarItemName="btnToMes" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
        <dxb:BarButtonItemLink BarItemName="btnToEdit" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
        <dxb:BarButtonItemLink BarItemName="btnCopyProp" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
        <dxb:BarButtonItemLink BarItemName="btnShowCalcValue" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
        <dxb:BarButtonItemLink BarItemName="btnDelete" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
        <dxb:BarButtonItemLink BarItemName="btnValidate" RibbonStyle="SmallWithText" UserGlyphSize="Small" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
        <dxb:BarButtonItemLink BarItemName="btnFr20Import" RibbonStyle="SmallWithText" UserGlyphSize="Small" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
        <dxb:BarButtonItemLink BarItemName="btnStatFlag" RibbonStyle="SmallWithText" UserGlyphSize="Small" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True"/>
        <dxb:BarButtonItemLink BarItemName="btnStZap" RibbonStyle="SmallWithText" UserGlyphSize="Small" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True"/>
        <dxb:BarButtonItemLink BarItemName="btnMesurCof" RibbonStyle="SmallWithText" UserGlyphSize="Small" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True"/>
        <dxb:BarButtonItemLink BarItemName="btnCopyApstProp" RibbonStyle="SmallWithText" UserGlyphSize="Small" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True"/>
        <dxb:BarButtonItemLink BarItemName="btnSiemens" RibbonStyle="SmallWithText" UserGlyphSize="Small" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True"/>
        <dxb:BarButtonItemLink BarItemName="btnAdgRpt" RibbonStyle="SmallWithText" UserGlyphSize="Small" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True"/>
        <dxb:BarButtonItemLink BarItemName="btnCopyApst4CmpCoil" RibbonStyle="SmallWithText" UserGlyphSize="Small" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True"/>
      </dxr:RibbonPageGroup>

      <dxr:RibbonPageGroup Name="rpgQuit" AllowCollapse="False">
        <dxb:BarButtonItemLink BarItemName="btnQuitxyz" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
      </dxr:RibbonPageGroup>
    </dxr:RibbonPage>
    <dxr:RibbonPage Caption="Аво->Лазер">
      <dxr:RibbonPageGroup Name="rpgAvoLasscr" Caption="Операции" AllowCollapse="False">
        <dxb:BarButtonItemLink BarItemName="btnS2LCheck" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
        <dxb:BarButtonItemLink BarItemName="btnS2LUnCheck" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
        <dxb:BarButtonItemLink BarItemName="btnS2LCpy" RibbonStyle="Large" UserGlyphSize="Large" BarItemDisplayMode="ContentAndGlyph" AllowDrop="True" />
      </dxr:RibbonPageGroup>
    </dxr:RibbonPage>
  </ruc:RibbonUserControl.UserPages>

  <dxl:LayoutControl x:Name="LayoutRoot" StretchContentVertically="True">
    <dxl:LayoutGroup Header="Образцы" View="GroupBox" ItemSpace="10" Width="600" HorizontalAlignment="Left" dxl:LayoutControl.AllowHorizontalSizing="True" Orientation="Vertical">
      <dxg:GridControl Name="GcSamples" ShowBorder="True" ItemsSource="{Binding Samples}">
        <dxg:GridControl.View>
          <dxg:TableView NavigationStyle="Row" AllowEditing="False" />
        </dxg:GridControl.View>
        <dxg:GridControl.Columns>
          <dxg:GridColumn FieldName="State" Header="Ст" MinWidth="25" Width="25">
            <dxg:GridColumn.CellTemplate>
              <DataTemplate>
                <StackPanel DataContext="{Binding RowData.Row}">
                  <Image Source="{Binding Path=State, Converter={StaticResource IntToImageConverter}}" Stretch="None" Width="17" Height="17" />
                </StackPanel>
              </DataTemplate>
            </dxg:GridColumn.CellTemplate>
          </dxg:GridColumn>
          <!--dxg:GridColumn Name="gridColumn7" FieldName="LaserFlag" Header="Л" MinWidth="20" Width="20" EditSettings="{dxe:CheckSettings}"/-->
          <dxg:GridColumn FieldName="SlFlg" Header="Л" MinWidth="25" Width="25">
            <dxg:GridColumn.CellTemplate>
              <DataTemplate>
                <StackPanel DataContext="{Binding RowData.Row}">
                  <Image Source="{Binding Path=SlFlg, Converter={StaticResource SlImageConverter}}" Stretch="None" Width="17" Height="17" />
                </StackPanel>
              </DataTemplate>
            </dxg:GridColumn.CellTemplate>
          </dxg:GridColumn>
          <dxg:GridColumn FieldName="IsQm" Header="ОП" MinWidth="27" Width="27" EditSettings="{dxe:CheckSettings}"/>
          <dxg:GridColumn FieldName="SampleNum"  Header="№ Образца" MinWidth="70" Width="70" ReadOnly="True" AllowEditing="False"/>
          <dxg:GridColumn FieldName="Line" Header="Агр-т" MinWidth="40" Width="40" ReadOnly="True" AllowEditing="False"/>
          <dxg:GridColumn FieldName="TestType" Header="Проба" MinWidth="40" Width="40" ReadOnly="True" AllowEditing="False"/>
          <dxg:GridColumn FieldName="SamplePos" Header="Т" MinWidth="20" Width="20" ReadOnly="True" AllowEditing="False"/>
          <dxg:GridColumn FieldName="SteelType" Header="Сталь" MinWidth="35" Width="35" ReadOnly="True" AllowEditing="False"/>
          <dxg:GridColumn FieldName="ThickNessNominal" Header="Толщ" MinWidth="35" Width="35" EditSettings="{dxe:TextSettings DisplayFormat=n2}"/>
          <dxg:GridColumn FieldName="MatLocalNumber" Header="Лок номер" MinWidth="75" Width="75"/>
          <dxg:GridColumn FieldName="MatMarkingInfo" Header="Стенд/Рул" MinWidth="80" Width="80"/>
          <dxg:GridColumn FieldName="DtSample" Header="Создан" MinWidth="100" Width="100" ReadOnly="True" AllowEditing="False">
            <dxg:GridColumn.EditSettings>
              <dxe:TextEditSettings DisplayFormat="dd.MM.yyyy HH:mm" />
            </dxg:GridColumn.EditSettings>
          </dxg:GridColumn>
          <dxg:GridColumn FieldName="DtSend" Header="Отправлен" MinWidth="100" Width="100">
            <dxg:GridColumn.EditSettings>
              <dxe:TextEditSettings DisplayFormat="dd.MM.yyyy HH:mm" />
            </dxg:GridColumn.EditSettings>
          </dxg:GridColumn>
          <dxg:GridColumn FieldName="UsrSample" Header="Кто отправил?" MinWidth="80" Width="80"/>
          <dxg:GridColumn FieldName="Md" Header="ОЗМ" MinWidth="120" Width="120"/>
          <dxg:GridColumn FieldName="SampleId" Header="ИД Обр." MinWidth="70" Width="70" ReadOnly="True" AllowEditing="False"/>
          <dxg:GridColumn FieldName="Tstep" Header="Т.Ш." MinWidth="70" Width="70" ReadOnly="True" AllowEditing="False"/>
          <dxg:GridColumn FieldName="IsValidate" Header="B" MinWidth="20" Width="20" EditSettings="{dxe:CheckSettings}"/>
          <dxg:GridColumn FieldName="StFlg" Header="Cт" MinWidth="23" Width="23" EditSettings="{dxe:CheckSettings}"/>
        </dxg:GridControl.Columns>
      </dxg:GridControl>
    </dxl:LayoutGroup>

    <dxl:LayoutGroup View="Group"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Orientation="Vertical">
            <dxl:LayoutGroup Header="Параметры образца" View="GroupBox" HorizontalAlignment="Stretch" Orientation="Vertical" VerticalAlignment="Top" Height="190">
                <dxl:LayoutGroup Header="Типы измерений параметров образца" View="GroupBox" HorizontalAlignment="Stretch" Orientation="Vertical">
                    <dxl:LayoutItem Name="layoutItem1">
                        <dxl:FlowLayoutControl Name="flowLayoutControl1" Orientation="Horizontal" ItemSpace="15" Padding="0">
                            <Button Command="{Binding ManualSetPropUnitSelectorCommand}" ToolTip="Занести результыты редактирования.">
                                <Image Source="pack://application:,,,/Viz.WrkModule.MagLab;component/Images/refresh-16x16.png" />
                            </Button>
                            <RadioButton Content="Лист" IsChecked="True" Command="{Binding SelectUnitCommand}" CommandParameter="1" Foreground="{Binding UnitColor[0], Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" IsEnabled="{Binding UnitEnable[0], Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <RadioButton Content="Эпштейн" Command="{Binding SelectUnitCommand}" CommandParameter="2" Foreground="{Binding UnitColor[1], Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" IsEnabled="{Binding UnitEnable[1], Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <RadioButton Content="Изоляция" Command="{Binding SelectUnitCommand}" CommandParameter="3" Foreground="{Binding UnitColor[2], Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" IsEnabled="{Binding UnitEnable[2], Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <RadioButton Content="Адгезия" Command="{Binding SelectUnitCommand}" CommandParameter="4" Foreground="{Binding UnitColor[3], Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" IsEnabled="{Binding UnitEnable[3], Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <RadioButton Content="Гибы" Command="{Binding SelectUnitCommand}" CommandParameter="5" Foreground="{Binding UnitColor[4], Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" IsEnabled="{Binding UnitEnable[4], Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                            <RadioButton Content="HИ" Command="{Binding SelectUnitCommand}" CommandParameter="6"/>
                        </dxl:FlowLayoutControl>
                    </dxl:LayoutItem>
                </dxl:LayoutGroup>
                <dxg:GridControl Name="GcSampleData" ItemsSource="{Binding SamplesData}" ShowBorder="True" Tag="1" AllowMRUFilterList="False">
                   <dxg:GridControl.Columns>
                        <dxg:GridColumn Name="grdDataCol1" FieldName="Massa" Header="Масса" Tag="1" MinWidth="55" Width="55" Visible="True" EditSettings="{dxe:TextSettings MaskType=Numeric, Mask=d, MaskUseAsDisplayFormat=True}" />
                        <dxg:GridColumn Name="grdDataCol2" FieldName="B3" Header="B3" Tag="9" MinWidth="55" Width="55" Visible="True" EditSettings="{dxe:TextSettings MaskType=Numeric, Mask=n2, MaskUseAsDisplayFormat=True}" />
                        <dxg:GridColumn Name="grdDataCol3" FieldName="B30" Header="B30" Tag="10" MinWidth="55" Width="55" Visible="True" EditSettings="{dxe:TextSettings MaskType=Numeric, Mask=n2, MaskUseAsDisplayFormat=True}" />
                        <dxg:GridColumn Name="grdDataCol4" FieldName="B100" Header="B100" Tag="2" MinWidth="55" Width="55" Visible="True" EditSettings="{dxe:TextSettings MaskType=Numeric, Mask=n2, MaskUseAsDisplayFormat=True}" />
                        <dxg:GridColumn Name="grdDataCol5" FieldName="B800" Header="B800" Tag="3" MinWidth="55" Width="55" Visible="True" EditSettings="{dxe:TextSettings MaskType=Numeric, Mask=n2, MaskUseAsDisplayFormat=True}" />
                        <dxg:GridColumn Name="grdDataCol6" FieldName="B2500" Header="B2500" Tag="4" MinWidth="55" Width="55" Visible="True" EditSettings="{dxe:TextSettings MaskType=Numeric, Mask=n2, MaskUseAsDisplayFormat=True}" />
                        <dxg:GridColumn Name="grdDataCol7" FieldName="B5000" Header="B5000" Tag="5" MinWidth="55" Width="55" Visible="True" EditSettings="{dxe:TextSettings MaskType=Numeric, Mask=n2, MaskUseAsDisplayFormat=True}" />
                        <dxg:GridColumn Name="grdDataCol8" FieldName="P1050" Header="P1.0/50" Tag="11" MinWidth="65" Width="65" Visible="True" EditSettings="{dxe:TextSettings MaskType=Numeric, Mask=n2, MaskUseAsDisplayFormat=True}" />
                        <dxg:GridColumn Name="grdDataCol9" FieldName="P1350" Header="P1.3/50" Tag="8" MinWidth="65" Width="65" Visible="True" EditSettings="{dxe:TextSettings MaskType=Numeric, Mask=n2, MaskUseAsDisplayFormat=True}" />
                        <dxg:GridColumn Name="grdDataCol10" FieldName="P1550" Header="P1.5/50" Tag="6" MinWidth="65" Width="65" Visible="True" EditSettings="{dxe:TextSettings MaskType=Numeric, Mask=n2, MaskUseAsDisplayFormat=True}" />
                        <dxg:GridColumn Name="grdDataCol11" FieldName="P1750" Header="P1.7/50" Tag="7" MinWidth="65" Width="65" Visible="True" EditSettings="{dxe:TextSettings MaskType=Numeric, Mask=n2, MaskUseAsDisplayFormat=True}" />
